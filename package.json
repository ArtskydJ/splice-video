{
  "name": "Splice-Video",
  "productName": "Splice Video",
  "version": "1.0.0",
  "description": "An electron app to quickly cut videos without re-encoding them, using the excellent FFMPEG",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ --overwrite --out=./dist --platform=darwin,linux,win32 --arch=ia32,x64 --icon=icon/icon"
  },
  "repository": "https://github.com/ArtskydJ/splice-video",
  "author": "ArtskydJ",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-packager": "^14.2.0",
    "electron-reloader": "^0.3.0"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.1.0"
  }
}
